Import('env')

env = env.Clone()
env.AppendUnique(CPPPATH=['../../..', '..'],
				 LIBPATH=['..'],
				 LIBS=['in_filtergen'],
				 CPPFLAGS=['-Dyyparse=filtergen_parse',
						   '-Dyydebug=filtergen_debug',
						   ])

env.Program('scan.c')
env.Program('parse.c')
env.Program('emit.c')
