set names [list \
	"foo" \
	"bar" \
	"baz" \
	"quux" \
	"mail.example.com" \
	]

spawn input/filtergen/t/scan

set timeout 3

foreach name $names {
	set test "$name identifier"

	send "$name\r"

	expect {
		"kind = identifier, spelling = \"$name\"" { pass $test }
		timeout { fail $test }
		eof { fail "$test (eof)" }
	}
}
